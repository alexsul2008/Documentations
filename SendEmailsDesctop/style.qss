QComboBox, QDateEdit
{
    background-color: transparent;
    border: 2px solid rgb(118, 121, 124);
    border-radius: 4px;
    padding: 5px;
    min-width: 100px;
}

QComboBox:open
{
    background-color: transparent;
    border: 2px solid rgb(118, 121, 124);
    cursor: pointer;
}

QComboBox:closed
{
    border: 2px solid rgb(118, 121, 124);
    cursor: pointer;
}

QComboBox:!enabled{
    color: rgb(118, 121, 124);
}

QComboBox:hover,
QComboBox:focus
{
    border: 2px solid rgb(61, 174, 233);
}

QComboBox:hover:pressed:!editable
{
    background-color: rgb(49, 54, 59);
}

QComboBox:hover:pressed:editable
{
   background-color: rgb(33, 47, 61);
}

QComboBox QAbstractItemView
{
    outline-color: 5px;
    cursor: pointer;
}

QComboBox QAbstractItemView::item {
    background-color: rgb(33, 47, 61);
    border: none;
    cursor: pointer;

    padding:3px;
    outline: 0px;
}

QComboBox QAbstractItemView::item:selected {
    background-color: rgb(42, 121, 163);
    cursor: pointer;
}
QComboBox QAbstractItemView::item:hover {
    background-color: rgb(42, 121, 163);
    cursor: pointer;
}

QComboBox::drop-down
{
    border: 0px;
    cursor: pointer;
    subcontrol-position: right top;
    subcontrol-origin:margin;
/*
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left-width: 1px;
    border-left-style: solid;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    cursor: pointer;
*/
}

QComboBox::down-arrow
{
    border-image: url(:/icons/icons/chevrons-down_.svg);
    width: 30px;
    height: 30px;
    margin-right: 15px;
    cursor: pointer;
}

QComboBox::down-arrow:on
{
    border-image: url(:/icons/icons/chevrons-up_.svg);
    width: 30px;
    height: 30px;
    margin-right: 15px;
    cursor: pointer;
}

