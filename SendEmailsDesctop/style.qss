QTabWidget::pane
{
    border: none;
    background-color: rgb(42, 61, 79);
}


/* TOP TABS */
QTabBar::tab:top,
QTabBar::tab:top:last,
QTabBar::tab:top:only-one
{
    color: #eff0f1;
    border: 5px transparent black;
    border-right: 0.04em solid rgb(118, 121, 124);
    border-top: 0.15em solid rgb(61, 174, 233);
    background-color: rgb(42, 61, 79);
    padding: 10px 20px;
    min-width: 50px;
    border-radius: 0.09em;
    border-bottom-left-radius: 0em;
    border-bottom-right-radius: 0em;
}

QTabBar::tab:top:!selected
{
    color: #eff0f1;
    background-color: rgb(33, 47, 61);
    border: 0.04em transparent black;
    border-right: 0.04em solid rgb(118, 121, 124);
    border-bottom: 0.04em solid rgb(118, 121, 124);
    border-top: 0.15em solid rgb(34, 48, 62);
    border-radius: 0.09em;
    border-bottom-left-radius: 0em;
    border-bottom-right-radius: 0em;
}

QTabBar::tab:top:next-selected
{
    border-right: 0.04em transparent rgb(44, 48, 52);
    border-bottom-left-radius: 0em;
    border-bottom-right-radius: 0em;
}

QTabBar::tab:top:!selected:hover
{
    background-color: rgba(61, 173, 232, 0.1);
    border-top: 0.15em solid rgb(42, 61, 79);
    border-radius: 0.09em;
    border-bottom-left-radius: 0em;
    border-bottom-right-radius: 0em;
}

QTabBar::tab:top:!selected:first:hover
{
    background-color: rgba(61, 173, 232, 0.1);
    border-radius: 0.09em;
    border-bottom-left-radius: 0em;
    border-bottom-right-radius: 0em;
}

QTableView::item:selected { background-color: rgb(33, 47, 61); font-weight:400; }
QTableCornerButton::section { background-color: red; }
QHeaderView::section { background-color: rgb(33, 47, 61); }

QScrollArea {
